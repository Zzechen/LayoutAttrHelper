<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.zzc.layoutattrhelper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Day">
        <activity android:name=".MainActivity" android:label="Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ShapeActivity" android:label="Shape"/>
        <activity android:name=".SelectorActivity" android:label="Selector"/>
        <activity android:name=".ThemeResActivity" android:label="ThemeRes"/>
        <activity android:name=".ThemeFileActivity" android:label="ThemeFile"/>
    </application>

</manifest>